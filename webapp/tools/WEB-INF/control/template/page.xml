<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE page-setting[
<!ELEMENT page-setting (css-files?,jscript-files?,pages?)>
<!ELEMENT css-files (css-file+)>
<!ELEMENT jscript-files (jscript-file+)>
<!ELEMENT pages (page+)>
<!ELEMENT page (enterkey-orders?)>
<!ELEMENT enterkey-orders (enterkey-order+)>
<!ATTLIST css-file 
  path CDATA #REQUIRED>
<!ATTLIST jscript-file 
  path CDATA #REQUIRED>
<!ATTLIST page
  path CDATA #REQUIRED
  resource-key CDATA #REQUIRED>
<!ATTLIST enterkey-order
  order NMTOKEN #REQUIRED
  control-id CDATA #REQUIRED>
]>
<page-setting>
  <css-files>
    <css-file path="/css/common.css" />
    <css-file path="/css/listcell.css" />
    <css-file path="/css/tool.css" />
  </css-files>
  <jscript-files>
    <jscript-file path="/jscript/user.js" />
  </jscript-files>
  <pages>
    <page path="/mainmenu/MainMenu" resource-key="TLE-T0001">
      <enterkey-orders>
        <enterkey-order order="1" control-id="txt_MenuResourceKey" />
        <enterkey-order order="2" control-id="rdo_EnableClick" />
      </enterkey-orders>
    </page>
    <page path="/mainmenulist/MainMenuList" resource-key="TLE-T0002">
      <enterkey-orders />
    </page>
    <page path="/listbox/resourcelist/ResourceList" resource-key="TLE-T0003">
      <enterkey-orders />
    </page>
    <page path="/mainmenuorder/MainMenuOrder" resource-key="TLE-T0004">
