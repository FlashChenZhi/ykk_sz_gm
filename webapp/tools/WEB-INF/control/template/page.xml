        <enterkey-order order="6" control-id="txt_Belonging" />
        <enterkey-order order="7" control-id="txt_BirthDate" />
        <enterkey-order order="8" control-id="txt_Note" />
      </enterkey-orders>
    </page>
    <page path="/listbox/userlist/UserList" resource-key="TLE-T0015">
