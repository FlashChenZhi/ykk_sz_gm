package jp.co.daifuku.wms.base.utility;


//#CM687005
/**
 * terminate fixed time processing
 */
public class Wait
{
    //#CM687006
    /**
     * main method
     */
	public static void main(String argv[])
	{
		try
		{
			long WaitTime = Integer.parseInt(argv[0]) * 1000;
			Thread.sleep(WaitTime) ;
		}
		catch(Exception e)
		{
		}
	}
}
