	/**
	 * 
	 * @param e
	 *            ActionEvent
	 * @throws Exception
	 */
	public void lst_ItemView_Click(ActionEvent e) throws Exception
	{
		int row = Integer.parseInt(e.getEventArgs().get(0).toString());
		int col = Integer.parseInt(e.getEventArgs().get(1).toString());

		if (col == listProxy.getNO_COLUMN())
		{
			lst_ItemView.setCurrentRow(row);

			session.setAttribute(ITEM_CODE, listProxy.getItemCode());
			session.setAttribute(ITEM_NAME_1, listProxy.getItemName1());
			session.setAttribute(ITEM_NAME_2, listProxy.getItemName2());
			session.setAttribute(ITEM_NAME_3, listProxy.getItemName3());
			ForwardParameters param = new ForwardParameters();
			parentRedirect(param);
		}
	}

