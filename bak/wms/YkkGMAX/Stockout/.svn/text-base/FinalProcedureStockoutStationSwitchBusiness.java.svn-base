	public void page_ConfirmBack(ActionEvent e) throws Exception
	{

		if (!this.getViewState().getBoolean(DIALOG_FLAG))
		{
			return;
		}
		boolean isExecute = new Boolean(e.getEventArgs().get(0).toString())
				.booleanValue();
		if (!isExecute)
		{
			return;
		}
		message.setMsgResourceKey("7000034");
		ArrayList list = new ArrayList();
		
		FinalProcedureStockoutStationSwitchEntity entity = new FinalProcedureStockoutStationSwitchEntity();
		
		entity.setRetrievalStationName(txt_CurrentStcokoutStation31.getText());
		entity.setStationNo(pul_ChangedStockoutStation31.getSelectedValue());
		entity.setFromStationNo("31");
		
		list.add(entity);
		
		entity = new FinalProcedureStockoutStationSwitchEntity();
		
		entity.setRetrievalStationName(txt_CurrentStcokoutStation32.getText());
		entity.setStationNo(pul_ChangedStockoutStation32.getSelectedValue());
		entity.setFromStationNo("32");
		
		list.add(entity);
		
		entity = new FinalProcedureStockoutStationSwitchEntity();
		
		entity.setRetrievalStationName(txt_CurrentStcokoutStation41.getText());
		entity.setStationNo(pul_ChangedStockoutStation41.getSelectedValue());
		entity.setFromStationNo("41");
		
		list.add(entity);
		
		entity = new FinalProcedureStockoutStationSwitchEntity();
		
		entity.setRetrievalStationName(txt_CurrentStcokoutStation42.getText());
		entity.setStationNo(pul_ChangedStockoutStation42.getSelectedValue());
		entity.setFromStationNo("42");
		
		list.add(entity);
		
		ProcessorInvoker pi = new ProcessorInvoker(message);
		
		pi.addProcessor(new FinalProcedureStockoutStationSwitchRequestProcessor(list));
		
		pi.run();
		
		page_Load(null);
