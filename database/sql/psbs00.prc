	D_FNHANSO_UPD.SAGYOKBN := WK_SAGYOKBN ;


	IF IN_SYORIKBN = SYORIKBN_NYUKO THEN





		D_FNHANSO_UPD.NYUSYUKBN := PA_AWC_DEFINE.NYUSYUKBN_NYU	;
