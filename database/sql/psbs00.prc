DBMS_OUTPUT.PUT_LINE('          WK_CARRY_QTY ['|| TO_CHAR (WK_CARRY_QTY) || ']' );

		PSBS00_A0 (IO_RC, WK_LOGMSG) ;
		IF IO_RC != FUKKI.RET_OK THEN
			GOTO	PSBS00_80ENDLABEL ;
		END IF ;




		PSBS00_D0 (IO_RC, WK_LOGMSG) ;
		IF IO_RC != FUKKI.RET_OK THEN
			GOTO	PSBS00_80ENDLABEL ;
		END IF ;




		PSBS00_B0 (PA_AWC_DEFINE.SAGYOKBN_IKISAKISIJI, SYORIKBN_CHOKO, IO_RC, WK_LOGMSG) ;

		IF IO_RC != FUKKI.RET_OK THEN
